{% extends 'base.html.twig' %}
{% block topmenu %}
    {{ dump(app.user) }}
    <a href="{{ path('admin_logout') }}"> Logout </a>
{% endblock %}
{% block body %}


    <section id="vendoredit-reg" class="container">
        <div class="panel panel-default panel-login">
            <div class="panel-heading text-center">
                <h2 class="panel-heading-text">
                    {{ 'Admin'|trans }} :: {% block admin_subheading %} {{ 'Home'|trans }} {% endblock %}
                </h2>
            </div>
            <div class="panel-body">
                <div class="row">
                    <div class="col-md-3 col-lg-3 col-xs-12 col-sm-3">
                        <a href="{{ url('admin_customers') }}">{{ 'Manage Customers'|trans }}</a><br />
                        <a href="{{ url('admin_companies') }}">{{ 'Manage Companies'|trans }}</a><br />
                        <a href="{{ url('admin_logout') }}">{{ 'Logout'|trans }}</a>

                    </div>
                    <div class="col-md-9 col-lg-9 col-xs-12 col-sm-3">
                        {% block adminbody %}{% endblock %}
                    </div>

                </div>
            </div>
        </div>
    </section>
    <div id="wrapper">
        <div id="container">
            <div id="welcome">
            </div>


        </div>
    </div>
{% endblock %}

{% block stylesheets %}

{% endblock %}
